{
  "name": "course-tools",
  "version": "1.0.0",
  "description": "Course Tools",
  "private": true,
  "author": "JunzhaoSun",
  "license": "ISC",
  "main": "main.js",
  "scripts": {
    "start": "npm run start:web",
    "start:web": "webpack serve -c compile/webpack.config.dev.js",
    "start:dev": "concurrently \"cross-env BROWSER=none npm run start:web\" \"wait-on http://localhost:7703 && cross-env NODE_ENV=development npm run run:ele\"",
    "start:pre": "cross-env NODE_ENV=pre npm run run:ele",
    "run:ele": "electron --trace-warnings .",
    "pack:mac": "electron-packager . --platform=darwin --arch=x64 --icon=./public/assets/icons/favicon.icns --asar --out ./build --app-version=1.0.0 --electron-version 14.0.0 --ignore=\"(node_modules|src|build|Makefile|releases|public|test|coverage|back|webpack.config.*.js|.gitignore|package-lock.json|README.md|app-server.js|serverWorker.js|.gitlab-ci.yml|craco.config.js|zip.js)\" --overwrite",
    "pack:linux": "electron-packager . --platform=linux --arch=x64 --icon=./public/assets/icons/favicon.ico --asar --out ./build --app-version=1.0.0 --electron-version 14.0.0 --ignore=\"(node_modules|src|build|Makefile|releases|public|test|coverage|back|webpack.config.*.js|.gitignore|package-lock.json|README.md|app-server.js|serverWorker.js|.gitlab-ci.yml|craco.config.js|zip.js)\" --overwrite",
    "pack:win": "electron-packager . --platform=win32 --arch=x64 --icon=./public/assets/icons/favicon.ico --asar --out ./build --app-version=1.0.0 --electron-version 14.0.0 --ignore=\"(node_modules|src|build|Makefile|releases|public|test|coverage|back|webpack.config.*.js|.gitignore|package-lock.json|README.md|app-server.js|serverWorker.js|.gitlab-ci.yml|craco.config.js|zip.js)\" --overwrite",
    "zip:linux": "cd build && node ../zip.js ../releases/1.0.0/Course-NeciBook-linux-x64-1.0.0.zip Course-NeciBook-linux-x64/",
    "zip:mac": "cd build && node ../zip.js ../releases/1.0.0/Course-NeciBook-darwin-x64-1.0.0.zip Course-NeciBook-darwin-x64/",
    "zip:win": "cd build && node ../zip.js ../releases/1.0.0/Course-NeciBook-win32-x64-1.0.0.zip Course-NeciBook-win32-x64/",
    "build:dev": "cross-env NODE_ENV=development webpack --config compile/webpack.config.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config compile/webpack.config.prod.js",
    "build:mac": "cross-env NODE_ENV=production ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ npm run pack:mac",
    "build:win": "cross-env NODE_ENV=production ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ npm run pack:win",
    "build:linux": "cross-env NODE_ENV=production ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ npm run pack:linux",
    "build": "npm run build:mac && npm run zip:mac && rm -rf build"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-proposal-private-methods": "^7.17.12",
    "@babel/plugin-proposal-private-property-in-object": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.18.3",
    "@kbox/indexed-db": "^1.0.4",
    "@webpack-cli/serve": "^1.6.1",
    "archiver": "^5.3.1",
    "asar": "^3.1.0",
    "babel-loader": "^8.2.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^2.1.1",
    "electron": "14.0.0",
    "electron-packager": "^15.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "html2canvas": "^1.4.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "shelljs": "^0.8.5",
    "simple-progress-webpack-plugin": "^2.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.1.0",
    "url-loader": "^1.1.2",
    "wait-on": "^6.0.1",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ali-oss": "^6.17.1",
    "antd": "^5.3.0",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.19.0",
    "jszip": "^3.10.1",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "path-webpack": "^0.0.3",
    "prop-types": "^15.8.1",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
